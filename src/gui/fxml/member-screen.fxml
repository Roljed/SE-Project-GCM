<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="320.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.MemberScreen">
   <children>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="83.0" layoutY="14.0" text="Member Menu">
         <font>
            <Font name="Gisha" size="24.0" />
         </font>
      </Label>
      <Label fx:id="welcomeMessage" alignment="CENTER" contentDisplay="CENTER" layoutX="112.0" layoutY="43.0" text="Welcome Message" textFill="#575757">
         <font>
            <Font name="Forte" size="12.0" />
         </font>
      </Label>
      <VBox alignment="CENTER" layoutX="60.0" layoutY="89.0" prefHeight="320.0" prefWidth="200.0" spacing="15.0">
         <children>
            <Button mnemonicParsing="false" onAction="#searchButton" text="Search Catalog" />
            <Button mnemonicParsing="false" onAction="#viewPurchasesButton" text="View Purchased Items" />
            <Button mnemonicParsing="false" onAction="#userInfoButton" text="Member Info" />
            <Button mnemonicParsing="false" onAction="#logoutButton" text="Logout">
               <VBox.margin>
                  <Insets top="30.0" />
               </VBox.margin>
            </Button>
            <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#exitButton" style="-fx-background-color: RED;" text="Exit" textFill="WHITE" />
         </children>
      </VBox>
   </children>
</AnchorPane>
